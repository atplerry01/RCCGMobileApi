{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "es2017",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "types": [
         "node"
      ]
   },
   "include": [ "./src/**/*.ts", "src/services/prayerwall copy.txt" ],
   "exclude": [ "node_modules" ]
}